# Default environment variable definitions are defined in makefile.defaults
 
include $(TRILINOS_HOME)/build/makefile.defaults

# Define architecture specific flags for C++, C and Fortran

ARCH_CXXFLAGS= -O3 
ARCH_CFLAGS= -O3 
ARCH_FFLAGS= -O3

# Define compiler commands

CC=/usr/local/bin/i386-mingw32msvc-gcc
FC=/usr/local/bin/i386-mingw32msvc-g77
CXX=/usr/local/bin/i386-mingw32msvc-g++
AR=/usr/local/bin/i386-mingw32msvc-ar

#Define include directories (remember to use -Idirectory_name)

MPI_INCLUDES = 
ARCH_INCLUDES = $(MPI_INCLUDES)

LIBMPI =
ARCH_LIBS = -lm -lg2c $(MPI_LIBS)

################## External Libraries ##########################

#LIBBLAS = /usr/lib/libblas.a
#LIBBLAS = /usr/lib/libblas_fast.a

LIBLAPACK = /Net/Proj/Xyce/arch/mingw/lib/epetra/liblapack.a
#LIBSPBLAS = /Net/Proj/Xyce/arch/mingw/lib/epetra/libspblas.a
LIBBLAS = /Net/Proj/Xyce/arch/mingw/lib/epetra/libblas.a
LIBY12M   = /Net/Proj/Xyce/arch/mingw/lib/epetra/liby12m.a
