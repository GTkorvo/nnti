TARGET=TSFExtendedSAND
BIBFILES= references.bib
FIGFILES =

$(TARGET).dvi: $(TARGET).tex $(BIBFILES) $(FIGFILES) TSFExtendedBody.tex DesignOverview.tex
	latex $(TARGET)
	bibtex $(TARGET)
	latex $(TARGET)
	latex $(TARGET)

view: $(TARGET).dvi
	xdvi $(TARGET) &

ps: $(TARGET).dvi
	dvips -o $(TARGET).ps $(TARGET)

pdf: ps
	ps2pdf $(TARGET).ps $(TARGET).pdf

print: $(TARGET).dvi
	dvips $(TARGET)

clean:
	rm -f $(TARGET).ps $(TARGET).pdf $(TARGET).dvi $(TARGET).aux $(TARGET).toc \
           $(TARGET).idx $(TARGET).ind $(TARGET).ilg $(TARGET).log $(TARGET).out \
           $(TARGET).bbl $(TARGET).blg $(TARGET).lof $(TARGET).lot $(TARGET).tex~
