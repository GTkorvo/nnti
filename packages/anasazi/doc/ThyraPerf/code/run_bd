#!/bin/csh

# sample script for running (serial) BlockDavidson tester
# for all three implementation, three times each

./BlockDavidson1DQ1_e.exe  $1  >> logs/BD$1_e.log
./BlockDavidson1DQ1_e.exe  $1  >> logs/BD$1_e.log
./BlockDavidson1DQ1_e.exe  $1  >> logs/BD$1_e.log

./BlockDavidson1DQ1_t.exe  $1  >> logs/BD$1_t.log
./BlockDavidson1DQ1_t.exe  $1  >> logs/BD$1_t.log
./BlockDavidson1DQ1_t.exe  $1  >> logs/BD$1_t.log

./BlockDavidson1DQ1_te.exe $1  >> logs/BD$1_te.log
./BlockDavidson1DQ1_te.exe $1  >> logs/BD$1_te.log
./BlockDavidson1DQ1_te.exe $1  >> logs/BD$1_te.log
