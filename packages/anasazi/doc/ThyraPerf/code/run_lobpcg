#!/bin/csh

# sample script for running (serial) LOBPCG tester
# for all three implementation, three times each

./LOBPCG1DQ1_e.exe  $1  >> logs/LOBPCG$1_e.log
./LOBPCG1DQ1_e.exe  $1  >> logs/LOBPCG$1_e.log
./LOBPCG1DQ1_e.exe  $1  >> logs/LOBPCG$1_e.log

./LOBPCG1DQ1_t.exe  $1  >> logs/LOBPCG$1_t.log
./LOBPCG1DQ1_t.exe  $1  >> logs/LOBPCG$1_t.log
./LOBPCG1DQ1_t.exe  $1  >> logs/LOBPCG$1_t.log

./LOBPCG1DQ1_te.exe $1  >> logs/LOBPCG$1_te.log
./LOBPCG1DQ1_te.exe $1  >> logs/LOBPCG$1_te.log
./LOBPCG1DQ1_te.exe $1  >> logs/LOBPCG$1_te.log
