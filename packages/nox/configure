      ;;
    esac

  ;;

  # SGI/IRIX test
  mips-sgi-irix* )

    if test "${CXX}" = "CC"; then
     CXXFLAGS_MISC="-LANG:std -LANG:ansi-for-init-scope=ON -64 -DSGI64"
     F77_MISC=f77
     FFLAGS_MISC="-64"
     SPECIAL_AR="CC -ar -W,l -W,s -o"
    fi

  ;;

  # Solaris test
  sparc-sun-solaris* )

