
@HAVE_IFPACK_AMESOS_TRUE@AMESOS_INCLUDES= -I$(top_builddir)/../amesos/src -I$(top_srcdir)/../amesos/src
@HAVE_IFPACK_AMESOS_TRUE@AMESOS_LIB= $(top_builddir)/../amesos/src/libamesos.a

@HAVE_IFPACK_AZTECOO_TRUE@AZTECOO_INCLUDES= -I$(top_builddir)/../aztecoo/src -I$(top_srcdir)/../aztecoo/src
@HAVE_IFPACK_AZTECOO_TRUE@AZTECOO_LIB= $(top_builddir)/../aztecoo/src/libaztecoo.a

@HAVE_IFPACK_TEUCHOS_TRUE@TEUCHOS_INCLUDES= -I$(top_builddir)/../teuchos/src -I$(top_srcdir)/../teuchos/src
@HAVE_IFPACK_TEUCHOS_TRUE@TEUCHOS_LIB= $(top_builddir)/../teuchos/src/libteuchos.a

IFPACK_INCLUDES= -I$(top_builddir)/src -I$(top_srcdir)/src \
	-I$(top_builddir)/../epetra/src -I$(top_srcdir)/../epetra/src \
	$(AZTECOO_INCLUDES) \
	$(TEUCHOS_INCLUDES) \
	$(AMESOS_INCLUDES)

IFPACK_LIBS= $(top_builddir)/src/libifpack.a \
	$(TEUCHOS_LIB) \
	$(AMESOS_LIB) \
	$(AZTECOO_LIB) \
	$(top_builddir)/../epetra/src/libepetra.a
