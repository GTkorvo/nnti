#!/bin/tcsh
setenv JAVACODE_HOME /home/mheroux/java
#setenv JAVACODE_HOME /usr/people/faculty/math/mheroux/java
setenv JLAPACK_HOME $JAVACODE_HOME/jlapack-0.6
setenv JPETRA_HOME $JAVACODE_HOME/JPetra/JPetra
setenv ALLMATVIS_HOME $JAVACODE_HOME/javacode/
setenv MATVIS_HOME $ALLMATVIS_HOME/matvis
setenv JPEG_HOME $ALLMATVIS_HOME/jpegencoder
setenv MYIO_HOME $ALLMATVIS_HOME/myio

setenv CLASSPATH .:$JLAPACK_HOME/blas.jar:$JLAPACK_HOME/blas_simple.jar:$JLAPACK_HOME/f2jutil.jar:$JLAPACK_HOME/lapack.jar:$JLAPACK_HOME/lapack_simple.jar:$JLAPACK_HOME/xerbla.jar:$JPETRA_HOME/:$MATVIS_HOME/:$JPEG_HOME/:MYIO_HOME/

echo Compiling CGDriver Example
javac CGDriver.java CGSolver.java
