#!/bin/tcsh 
setenv JLAPACK_HOME /home/mheroux/java/jlapack-0.6
#setenv JLAPACK_HOME /usr/people/faculty/math/mheroux/java/jlapack-0.6
setenv CLASSPATH .:$JLAPACK_HOME/blas.jar:$JLAPACK_HOME/blas_simple.jar:$JLAPACK_HOME/f2jutil.jar:$JLAPACK_HOME/lapack.jar:$JLAPACK_HOME/lapack_simple.jar:$JLAPACK_HOME/xerbla.jar

echo Compiling JPetra class $1
javac $1.java
mv $1.class ../JPetra
