#!/bin/sh
echo
echo 'This should be run from the ml root directory'
echo
cd lib
cd Aztec/lib ; pwd; make -f Makefile.template clean; rm libaztec.a ; cd ../..
cd DSuperLU/SRC; make clean; rm ../superlu*.a ; cd ../..
cd ParMetis-2.0/METISLib; make clean; rm ../libmetis.a ; cd ../..
cd ParaSails ; make clean; cd ..
if test -d Obj 
then
  cd ../Obj; make clean; cd ..
fi

