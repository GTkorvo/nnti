  # Base make configuration
MSQ_BASE_DIR = @MSQ_BASE_DIR@
PREFIX       = @
DEBUG_FLAG   = 

  # Programs
CXX        = @CXX@
LINKER     = $(CXX)
ARCHIVER   = @CXX_ARCHIVER@
MAKEDEPEND = @MAKEDEPEND@
BABEL      = @BABEL@
DOXYGEN    = @DOXYGEN@

  # Flags
CXXFLAGS   = @CXXFLAGS@ $(DEBUG_FLAG)
LDFLAGS    = @LDFLAGS@
FUNCDEFS   = @MSQ_FUNC_DEFS@
CPPUNIT_DIR = @CPPUNIT_DIR@
CPPUNIT_INC = @CPPUNIT_INC@
CPPUNIT_LNK = @CPPUNIT_LNK@

  # ExodusII
EXODUS_DIR     = @EXODUS_DIR@
NETCDF_DIR     = @NETCDF_DIR@
EXODUS_CFLAGS  = @EXODUS_CFLAGS@
EXODUS_LDFLAGS = @EXODUS_LDFLAGS@
NETCDF_LNK     = @NETCDF_LNK@

  # TSTT
MSQ_TSTT_MESH  = @MSQ_TSTT_MESH@
MSQ_TSTT_GEOM  = @MSQ_TSTT_GEOM@
SIDL_DIR       = @SIDL_DIR@
SIDL_INC       = @SIDL_INC@
SIDL_LNK       = @SIDL_LNK@

  # Stuff for TSTT tests
MSQ_TSTT_BASE_IMPL = @MSQ_TSTT_BASE_IMPL@
MSQ_TSTT_MESH_IMPL = @MSQ_TSTT_MESH_IMPL@
MSQ_TSTT_BASE_DEFS = @MSQ_TSTT_BASE_DEFS@
MSQ_TSTT_MESH_DEFS = @MSQ_TSTT_MESH_DEFS@
ifeq ($(MSQ_TSTT_MESH_IMPL),yes)
  include $(MSQ_TSTT_BASE_DEFS)
  include $(MSQ_TSTT_MESH_DEFS)
endif

  # Combine
CONFIG_CFLAGS = $(FUNCDEFS) $(EXODUS_CFLAGS) $(INCLUDE)
CONFIG_LDFLAGS = $(EXODUS_LDFLAGS) $(NETCDF_LNK)
#CXXFLAGS += $(CONFIG_CFLAGS)
#LDFLAGS += $(CONFIG_LDFLAGS)
