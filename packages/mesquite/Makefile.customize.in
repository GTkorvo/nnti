  # Base make configuration
MSQ_BASE_DIR = @MSQ_BASE_DIR@
PREFIX       = @
DEBUG_FLAG   = 

  # Programs
CXX        = @CXX@
LINKER     = $(CXX)
ARCHIVER   = @CXX_ARCHIVER@
MAKEDEPEND = @MAKEDEPEND@
BABEL      = @BABEL@
DOXYGEN    = @DOXYGEN@

  # Flags
CXXFLAGS   = @CXXFLAGS@ $(DEBUG_FLAG)
LDFLAGS    = @LDFLAGS@
FUNCDEFS   = @MSQ_FUNC_DEFS@ @HAVE_IEEEFP@
CPPUNIT_INC = @CPPUNIT_INC@
CPPUNIT_LNK = @CPPUNIT_LNK@

  # ExodusII
EXODUS_DIR     = @EXODUS_DIR@
NETCDF_DIR     = @NETCDF_DIR@
EXODUS_CFLAGS  = @EXODUS_CFLAGS@
EXODUS_LDFLAGS = @EXODUS_LDFLAGS@
NETCDF_LNK     = @NETCDF_LNK@
NETCDF_CXX_LNK = @NETCDF_CXX_LNK@

  # TSTT
MSQ_TSTT_MESH  = @MSQ_TSTT_MESH@
MSQ_TSTT_GEOM  = @MSQ_TSTT_GEOM@
SIDL_DIR       = @SIDL_DIR@
SIDL_INC       = @SIDL_INC@
SIDL_LNK       = @SIDL_LNK@

  # Stuff for TSTT tests

  # General
TSTT_MESH_IMPL    = @TSTT_MESH_IMPL@
TSTT_MESH_CFLAGS  = $($(TSTT_MESH_IMPL)_INC) $(SIDL_INC) -DMSQ_TSTT_USE_$(TSTT_MESH_IMPL)=1
TSTT_MESH_LDFLAGS = $($(TSTT_MESH_IMPL)_LNK) $(SIDL_LNK)

  # AOMD
AOMD_DIR       = @AOMD_DIR@
AOMD_INC       = @AOMD_INC@
AOMD_LNK       = @AOMD_LNK@

  # MOAB
HDF5_DIR       = @HDF5_DIR@
HDF5_LNK       = @HDF5_LNK@
MOAB_DIR       = @MOAB_DIR@
MOAB_INC       = @MOAB_INC@
MOAB_LNK       = @MOAB_LNK@

  # Overture
OVERTURE_DIR   = @OVERTURE_DIR@
OVERTURE_INC   = @OVERTURE_INC@
OVERTURE_LNK   = @OVERTURE_LNK@


  # Combine
CONFIG_CFLAGS = $(FUNCDEFS) $(EXODUS_CFLAGS) $(INCLUDE)
CONFIG_LDFLAGS = $(EXODUS_LDFLAGS)
#CXXFLAGS += $(CONFIG_CFLAGS)
#LDFLAGS += $(CONFIG_LDFLAGS)
