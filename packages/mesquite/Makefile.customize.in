  # Base make configuration
MSQ_BASE_DIR = @MSQ_BASE_DIR@
PREFIX       = @
DEBUG_FLAG   = 

  # Programs
CXX        = @CXX@
LINKER     = $(CXX)
ARCHIVER   = @CXX_ARCHIVER@
MAKEDEPEND = @MAKEDEPEND@
BABEL      = @BABEL@
DOXYGEN    = @DOXYGEN@
SED        = @SED@

  # Flags
CXXFLAGS   = @CXXFLAGS@
LDFLAGS    = @LDFLAGS@
FUNCDEFS   = @MSQ_FUNC_DEFS@ @HAVE_IEEEFP@
CPPUNIT_INC = @CPPUNIT_INC@
CPPUNIT_LNK = @CPPUNIT_LNK@

  # ExodusII
EXODUS_CFLAGS   = @EXODUS_CFLAGS@
EXODUS_LDFLAGS  = @EXODUS_LDFLAGS@

  # TSTT
TSTT_CLIENT_DIR     = @TSTT_CLIENT_DIR@
TSTT_CFLAGS         = @TSTT_CFLAGS@
TSTT_LDFLAGS        = @TSTT_LDFLAGS@
TSTT_OBJS           = @TSTT_OBJS@

  # AOMD
AOMD_FLAG       = @AOMD_FLAG@

  # Combine
CONFIG_CFLAGS = $(DEBUG_FLAG) $(FUNCDEFS) $(EXODUS_CFLAGS) $(TSTT_CFLAGS) $(INCLUDE)
CONFIG_LDFLAGS = $(EXODUS_LDFLAGS) $(TSTT_LDFLAGS)
CXXFLAGS += $(CONFIG_CFLAGS)
LDFLAGS += $(CONFIG_LDFLAGS)
