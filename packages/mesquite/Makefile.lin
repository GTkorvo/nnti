## Default definitions for tools
CXX = g++
ARCHIVER = ar ru
MAKEDEPEND = makedepend
LINKER = ${CXX}

## Default settings for flags
DEBUG_FLAG = -g  -Wno-deprecated
STD_INCLUDE_FLAG = -DUSE_STD_INCLUDES -DUSE_C_PREFIX_INCLUDES
AOMD_FLAG = -D_MY_HASH_TABLE_
CXXFLAGS = ${DEBUG_FLAG} ${INCLUDE} ${STD_INCLUDE_FLAG} ${AOMD_FLAG}
DEPEND_FLAGS = ${DEBUG_FLAG} ${INCLUDE} ${STD_INCLUDE_FLAG}
LDFLAGS = ${DEBUG_FLAG}
