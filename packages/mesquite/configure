#-----------------------------------------------------------------------------
# Try to find an archiver that correctly handles C++ templates
#-----------------------------------------------------------------------------
CXX_ARCHIVER=

# Check whether --with-archiver was given.
if test "${with_archiver+set}" = set; then
  withval=$with_archiver;


src_file=conftest.cc
obj_file=conftest.o
lib_file=conftest.a
rm -f $obj_file $lib_file

echo "int main(int argc, char* argv) {return 0;}" > $src_file
if $CXX -c $src_file; then
  if ($withval $lib_file $obj_file && ar t $lib_file) > /dev/null 2>&1; then
     CXX_ARCHIVER=$withval;
  else
    { { echo "$as_me:$LINENO: error: \"$with_archiver\" : not a valid archiver." >&5
echo "$as_me: error: \"$with_archiver\" : not a valid archiver." >&2;}
   { (exit 1); exit 1; }; }

  fi
fi

rm -f $src_file $obj_file $lib_file
src_file=
obj_file=
lib_file=

else

{ echo "$as_me:$LINENO: checking for C++ archiver" >&5
echo $ECHO_N "checking for C++ archiver... $ECHO_C" >&6; }

  # Loop through list of possibilities...
CXX_ARCHIVER=
for i in "$CXX -xar -o" "$CXX -ar -o" "ar cr"; do
  if test -z "$CXX_ARCHIVER"; then


src_file=conftest.cc
obj_file=conftest.o
lib_file=conftest.a
rm -f $obj_file $lib_file

echo "int main(int argc, char* argv) {return 0;}" > $src_file
if $CXX -c $src_file; then
  if ($i $lib_file $obj_file && ar t $lib_file) > /dev/null 2>&1; then
    CXX_ARCHIVER="$i"

  fi
fi

rm -f $src_file $obj_file $lib_file
src_file=
obj_file=
lib_file=

  fi
done

if test -z "$CXX_ARCHIVER"; then
  { echo "$as_me:$LINENO: result: failed" >&5
echo "${ECHO_T}failed" >&6; }
  { { echo "$as_me:$LINENO: error: No C++ archiver." >&5
echo "$as_me: error: No C++ archiver." >&2;}
   { (exit 1); exit 1; }; }
else
  { echo "$as_me:$LINENO: result: $CXX_ARCHIVER" >&5
echo "${ECHO_T}$CXX_ARCHIVER" >&6; }

fi



fi



