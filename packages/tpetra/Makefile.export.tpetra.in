
@ENABLE_TPETRA_THYRA_TRUE@include @abs_top_builddir@/../thyra/Makefile.export.thyra

@ENABLE_TPETRA_THYRA_TRUE@TPETRA_THYRA_INCLUDES = \
@ENABLE_TPETRA_THYRA_TRUE@	-I@abs_top_srcdir@/thyra/src \
@ENABLE_TPETRA_THYRA_TRUE@	-I@abs_top_builddir@/thyra/src \
@ENABLE_TPETRA_THYRA_TRUE@	$(THYRA_INCLUDES)

@ENABLE_TPETRA_THYRA_TRUE@TPETRA_THYRA_LIBS = $(THYRA_LIBS)

_TPETRA_INCLUDES = \
  $(TPETRA_THYRA_INCLUDES) \
	-I@abs_top_srcdir@/../kokkos/src \
	-I@abs_top_builddir@/../kokkos/src \
	-I@abs_top_srcdir@/src \
	-I@abs_top_builddir@/src

# ToDo: Give kokkos its own export makefile and remove the explicit paths above!

_TPETRA_LIBS = @LDFLAGS@ -L@abs_top_builddir@/src $(TPETRA_THYRA_LIBS) -ltpetra \
  -L@abs_top_builddir@/../kokkos/src -lkokkos \
  $(TEUCHOS_LIBS) \
  @LAPACK_LIBS@ @BLAS_LIBS@ @LIBS@ @FLIBS@ 

@USING_GNUMAKE_TRUE@TPETRA_INCLUDES  = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_incl_paths.pl $(_TPETRA_INCLUDES))
@USING_GNUMAKE_TRUE@TPETRA_LIBS      = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_libs.pl $(_TPETRA_LIBS))
@USING_GNUMAKE_FALSE@TPETRA_INCLUDES = $(_TPETRA_INCLUDES)
@USING_GNUMAKE_FALSE@TPETRA_LIBS     = $(_TPETRA_LIBS)
